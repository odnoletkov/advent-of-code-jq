{
  "*.input": {
    "alternate": "{}.sample",
    "type": "input",
    "make": "make -B %",
    "start": ":vertical Jqplay -Rn"
  },
  "*.sample": {
    "alternate": "{}.input",
    "type": "sample",
    "make": "make -B %",
    "start": ":vertical Jqplay -Rn"
  },
  "*.jq": {
    "type": "jq",
    "make": "time make %:r"
  },
  "*.swift": {
    "dispatch": "swiftc -O -o %.out % && time %.out",
    "make": "time swift % && exit 1"
  }
}
